{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "plugins": [
        "angular"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1
            }
        ],
        "camelcase": 0
    }
}
